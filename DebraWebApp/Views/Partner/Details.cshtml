@model DebraWebApp.Models.Partner

<h2>Partner Details</h2>

<div>
    <h4>Partner</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Partner Id
        </dt>
        <dd class="col-sm-10">
            @Model.Id
        </dd>
        <dt class="col-sm-2">
            Name
        </dt>
        <dd class="col-sm-10">
            @Model.Name
        </dd>
        <dt class="col-sm-2">
            Email
        </dt>
        <dd class="col-sm-10">
            @Model.Email
        </dd>
        <dt class="col-sm-2">
            Phone
        </dt>
        <dd class="col-sm-10">
            @Model.PhoneNumber
        </dd>
        <dt class="col-sm-2">
            Address
        </dt>
        <dd class="col-sm-10">
            @Model.AddressNo, @Model.Street, @Model.City
        </dd>
    </dl>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>
