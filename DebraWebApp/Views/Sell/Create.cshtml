@model DebraWebApp.Models.Sell

<h1>Create Sell</h1>

@if (TempData["SuccessMessage"] != null)
{
    <div class="alert alert-success">
        @TempData["SuccessMessage"]
    </div>
}

<form asp-action="Create">
    <div class="form-group">
        <label asp-for="TicketId"></label>
        <input asp-for="TicketId" class="form-control" />
        <span asp-validation-for="TicketId" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="QuantitySold"></label>
        <input asp-for="QuantitySold" class="form-control" />
        <span asp-validation-for="QuantitySold" class="text-danger"></span>
    </div>

    <button type="submit" class="btn btn-primary">Create</button>
    <div>
        <a class="btn btn-primary" href="javascript:history.go(-2)">Go Back</a>
    </div>
</form>
